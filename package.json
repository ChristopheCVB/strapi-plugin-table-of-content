{
  "version": "1.0.2",
  "strapi": {
    "kind": "plugin",
    "name": "table-of-content",
    "displayName": "Table of Content",
    "description": "Display a Table of Content in EditView"
  },
  "name": "strapi-plugin-table-of-content",
  "description": "Display a Table of Content in EditView",
  "keywords": [
    "strapi",
    "plugin",
    "table-of-content"
  ],
  "author": {
    "name": "Christophe Carvalho Vilas-Boas",
    "url": "https://github.com/ChristopheCVB"
  },
  "maintainers": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    }
  ],
  "contributors": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristopheCVB/strapi-plugin-table-of-content"
  },
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "typecheck:front": "tsc --noEmit -p admin/tsconfig.json",
    "typecheck:back": "tsc --noEmit -p server/tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@strapi/content-manager": "^5.33.3",
    "@strapi/design-system": "^2.1.1",
    "@strapi/icons": "^2.1.1",
