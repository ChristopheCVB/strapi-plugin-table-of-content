    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.33.3",
    "@strapi/typescript-utils": "^5.33.3",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/koa": "^2.15.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "koa": "^2.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.33.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.5"
  },
  "engines": {
    "node": ">=22.21.1"
  },
  "packageManager": "pnpm@10.28.0",
  "volta": {
    "node": "22.21.1",
    "pnpm": "10.28.0"
  }
}
